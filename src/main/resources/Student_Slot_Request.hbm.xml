<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	 <class name="org.iiitb.courseproject.model.Student_Slot_Request" table="Student_Slot_Request">
	 
      <meta attribute = "class-description">
         This class contains the Date detail. 
      </meta>
      
      <id name = "idRequest" type = "int" column = "idRequest">
         <generator class="native"/>
      </id>
      
      <property name = "Student_id" column = "Student_id" type = "int"/>
      <property name = "Slot_id" column = "Slot_id" type = "int"/>
     
	</class>
	
</hibernate-mapping>